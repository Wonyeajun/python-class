def solution(numbers, target):
    arr = [0]
    for i in numbers:
        temp =[]
        for j in arr:
            temp.append(j+i)
            temp.append(j-i)
            print(f"arr : {arr}")
            print(f"temp : {temp}")
        arr = temp
        print(f"arr_tempted : {arr}")
    return arr.count(target)
print(solution([1, 1, 1, 1, 1], 3))
